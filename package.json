{
  "name": "1.-new",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node --env-file=.env index.js",
    "start:dev": "nodemon --env-file=.env index.js",
    "db:migrate:dev": "npx sequelize db:migrate --env development --config ./src/config/development/config.json",
    "db:migrate:dev:undo:all": "npx sequelize db:migrate:undo:all --env development --config ./src/config/development/config.json",
    "db:create:migration": "npx sequelize migration:create --name",
    "db:seed": "npx sequelize db:seed --env development --config ./src/config/development/config.json",
    "db:seed:undo": "npx sequelize db:seed:undo --env development --config ./src/config/development/config.json",
    "db:seed:undo:all": "npx sequelize db:seed:undo:all --env development --config ./src/config/development/config.json",
    "db:seed:all": "npx sequelize db:seed:all --env development --config ./src/config/development/config.json",
    "lint": "eslint .",
    "prepare": "husky",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "fastest-validator": "^1.19.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.6.1",
    "nodemon": "^3.1.9",
    "pg": "^8.14.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.1.0"
  }
}
